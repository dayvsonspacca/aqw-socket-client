{
    "name": "dayvsonspacca/aqw-socket-client",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "fmt": "./vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "autoload": {
        "psr-4": {
            "AqwSocketClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AqwSocketClient\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Dayvson Spacca",
            "email": "spacca.dayvson@gmail.com"
        }
    ],
    "require": {
        "react/socket": "^1.16"
    },
    "require-dev": {
        "symfony/var-dumper": "^7.3",
        "phpunit/phpunit": "^12.3",
        "friendsofphp/php-cs-fixer": "^3.88"
    },
    "minimum-stability": "stable"
}
